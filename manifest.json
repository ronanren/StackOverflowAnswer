{
  "name": "StackOverflow Answer",
  "description": "Quick answers of StackOverflow topics on Google",
  "version": "0.2",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "include_globs": [
        "http://www.google.*/*",
        "https://www.google.*/*"
      ],
      "js": [
        "js/jquery-3.6.0.min.js",
        "js/run_prettify.js",
        "content.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  }
}